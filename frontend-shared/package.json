{
  "name": "frontend-shared",
  "version": "1.0.0",
  "dependencies": {
    "cross-env": "5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/preset-env",
            ["@babel/preset-react", {
              "pragma": "createElement"
            }]
          ]
        }
      ], [
        "stringify",
        {
          "appliesTo": {
            "includeExtensions": [
              ".html",
              ".svg"
            ]
          }
        }
      ]
    ]
  }
}
