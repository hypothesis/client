{
  "presets": [
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "importSource": "preact"
      }
    ],
    [
      "@babel/preset-env",
      {
        "bugfixes": true,
        "targets": {
          "chrome": "57",
          "firefox": "53",
          "safari": "10.1",
          "edge": "17"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-react",
          {
            "development": true,
            "runtime": "automatic",
            "importSource": "preact/compat"
          }
        ]
      ]
    }
  }
}
