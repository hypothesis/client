{
  "presets": [
    ["@babel/preset-react", {
      "pragma": "createElement"
    }],

    // Compile JS for browser targets set by `browserslist` key in package.json.
    ["@babel/preset-env", {
       "bugfixes": true
    }]
  ],
  "plugins": [
    "angularjs-annotate"
  ],
  "ignore": ["**/vendor/*"],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", {
          "development": true,
          "pragma": "createElement"
        }]
      ]
    }
  }
}
