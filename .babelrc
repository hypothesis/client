{
  "presets": [
    ["@babel/preset-react", {
      "pragma": "createElement"
    }],
    ["@babel/preset-env", {
       "bugfixes": true,
       "targets": {
         "chrome": "55",
         "edge": "17",
         "firefox": "53",
         "safari": "10.1",
      }
    }]
  ],
  "plugins": [
    "angularjs-annotate"
  ],
  "ignore": ["**/vendor/*"],
  "env": {
    "development": {
      "presets": [
        ["@babel/preset-react", {
          "development": true,
          "pragma": "createElement"
        }]
      ]
    }
  }
}
